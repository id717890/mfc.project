{
  "name": "mfc-project",
  "version": "2.0.0",
  "description": "Приложение для автоматизации работы МФЦ",
  "scripts": {
    "test:once": "karma start karma.conf.js --single-run",
    "build": "ntsc -p app/",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "build:watch": "ntsc -p app/ -w",
    "build:upgrade": "ntsc",
    "serve:upgrade": "http-server",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "build:babel": "babel app -d app --extensions \".es6\" --source-maps",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n",
    "lint": "tslint ./app/**/*.ts -t verbose"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular2-busy": "^1.0.2",
    "angular2-dynamic-component": "0.0.50",
    "angular2-in-memory-web-api": "0.0.20",
    "angular2-modal": "2.0.0-beta.13",
    "base64-js": "^0.0.8",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "mydatepicker": "^1.8.3",
    "ng2-completer": "^0.2.3",
    "ng2-pagination": "^0.5.1",
    "ntypescript": "^1.201609302242.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "semantic-ui-loader": "^2.2.3",
    "systemjs": "0.19.27",
    "ts-metadata-helper": "0.0.4",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  },
  "repository": {
    
  }
}
